ns2:133_method dcterms:hasPart ns2:133_step0,
        ns2:133_step1,
        ns2:133_step10,
        ns2:133_step11,
        ns2:133_step12,
        ns2:133_step13,
        ns2:133_step14,
        ns2:133_step15,
        ns2:133_step16,
        ns2:133_step17,
        ns2:133_step18,
        ns2:133_step19,
        ns2:133_step2,
        ns2:133_step20,
        ns2:133_step21,
        ns2:133_step22,
        ns2:133_step23,
        ns2:133_step24,
        ns2:133_step3,
        ns2:133_step4,
        ns2:133_step5,
        ns2:133_step6,
        ns2:133_step7,
        ns2:133_step8,
        ns2:133_step9 .

ns2:133_step0 ns1:instruction "Preheat the oven to 400 degrees F (200 degrees C)" .

ns2:133_step1 ns1:instruction "Adjust an oven rack to the middle position" .

ns2:133_step10 ns1:instruction "Spread melted butter over dough; sprinkle with a generous amount of cinnamon sugar" .

ns2:133_step11 ns1:instruction "Place apple slices along one long edge of dough, about 1/4 inch beyond the edge, overlapping slices slightly" .

ns2:133_step12 ns1:instruction "Fold the bottom half of dough over apple slices to form a long \"folder\" of dough with rounded edges of apple slices exposed" .

ns2:133_step13 ns1:instruction "Beat together egg and water in a small bowl" .

ns2:133_step14 ns1:instruction "Brush surface of dough with egg wash" .

ns2:133_step15 ns1:instruction "Sprinkle with cinnamon sugar" .

ns2:133_step16 ns1:instruction "Starting at one end, roll dough to form a rose-shaped pastry" .

ns2:133_step17 ns1:instruction "Seal roll with end of dough strip" .

ns2:133_step18 ns1:instruction "Transfer roses to the prepared ramekins" .

ns2:133_step19 ns1:instruction "Sprinkle with cinnamon sugar" .

ns2:133_step2 ns1:instruction "Butter two (6 to 8-ounce) ramekins and dust with white sugar" .

ns2:133_step20 ns1:instruction "Bake on the middle rack in the preheated oven until well browned, about 45 minutes" .

ns2:133_step21 ns1:instruction "Use tongs to remove the ramekins to a baking sheet to cool for 5 to 10 minutes" .

ns2:133_step22 ns1:instruction "Remove apple roses from the ramekins and finish cooling on a wire rack" .

ns2:133_step23 ns1:instruction "Dust with confectioners' sugar before serving" .

